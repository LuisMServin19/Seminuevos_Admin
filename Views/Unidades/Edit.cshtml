@model Serfitex.Models.Unidades

@{
    ViewData["Title"] = "Edit User";
}

<h1>Editar</h1>

<h4>Distribuidor</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <input type="hidden" asp-for="Id_unidad" />
            <div class="form-group">
                <label asp-for="Modelo" class="control-label"></label>
                <input asp-for="Modelo" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Modelo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Marca" class="control-label"></label>
                <input asp-for="Marca" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Marca" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Num_serie" class="control-label"></label>
                <input asp-for="Num_serie" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Num_serie" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ano" class="control-label"></label>
                <input asp-for="Ano" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Ano" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Color" class="control-label"></label>
                <input asp-for="Color" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Color" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fecha_factura" class="control-label"></label>
                <input asp-for="Fecha_factura" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Fecha_factura" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fecha_tenencia" class="control-label"></label>
                <input asp-for="Fecha_tenencia" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Fecha_tenencia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Seguro" class="control-label"></label>
                <input asp-for="Seguro" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Seguro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Comentario" class="control-label"></label>
                <input asp-for="Comentario" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Comentario" class="text-danger"></span>
            </div>
            @* <div class="form-group">
                <label asp-for="Estatus" class="control-label"></label>
                <select asp-for="Estatus" class="form-control" required>
                    <option value="1">Disponible</option>
                    <option value="0">Vendido</option>
                </select>
                <span asp-validation-for="Estatus" class="text-danger"></span>
            </div> *@
            <div class="form-group">
                <label asp-for="Fecha_ingreso" class="control-label"></label>
                <input asp-for="Fecha_ingreso" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Fecha_ingreso" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fech_prox_tenecia" class="control-label"></label>
                <input asp-for="Fech_prox_tenecia" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Fech_prox_tenecia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fech_prox_verificacion" class="control-label"></label>
                <input asp-for="Fech_prox_verificacion" class="form-control" Autocomplete="off" />
                <span asp-validation-for="Fech_prox_verificacion" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<br/>
<div>
    <a asp-action="Index">Regresar a la lista</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
